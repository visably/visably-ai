            if (ecEv.event === 'addToCart' && ecEv.ecommerce && ecEv.ecommerce.add && ecEv.ecommerce.add.products) {
                for (var i = 0; i < ecEv.ecommerce.add.products.length; i++) {
                    pr = ecEv.ecommerce.add.products[i];
                    reporting.addToCart(pr.id, pr.quantity, pr.dimension1);
                }
            }

            var eventTypes = {'checkout' : 'checkout', 'transaction' : 'purchase'};
            for (var eventType in eventTypes) {
                var key = eventTypes[eventType];
                if (ecEv.event === eventType && ecEv.ecommerce && ecEv.ecommerce[key] && ecEv.ecommerce[key].products) {
                    var obj = {};
                    for (var i = 0; i < ecEv.ecommerce[key].products.length; i++) {
                        pr = ecEv.ecommerce[key].products[i];
                        if (pr.id) {
                            obj[pr.id] = pr.quantity;
                        }
                    }
                    if (Object.keys(obj).length) {
                        reporting[key + 'View'](obj);
                    }
                }
            }
        };
