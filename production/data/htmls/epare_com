    //clearTimeout(timer);
    var for_val = $(this).attr('for');
    pf_option1 = $("#"+for_val).val();
  });
  
  $('.option2 label').click(function() {
    //clearTimeout(timer);
    var for_val = $(this).attr('for');
    pf_option2 = $("#"+for_val).val();
  });
  
  $('#pf-o-1').change(function() {
    var val = $(this).val();
    pf_option1 = val;
    var text_val = $("#pf-o-1 option[value='"+val+"']").text();
    $("#selected-option-1").text(text_val);
    var select = $('#pf-o-1');
    switch(val) {
        case 'gift':
            select.css('padding-left', '30px');
            break;
        case 'present':
            select.css('display', 'none');
             break;
        default:
            return;
    }
  });

  $('#pf-o-1b').change(function() {
    var val = $(this).val();
    pf_option2 = val;
    var text_val = $("#pf-o-1b option[value='"+val+"']").text();
    $("#selected-option-2").text(text_val);
  });
 
//   $('#pf-o-1b, #pf-o-1, #discover-product').click(function() {
//     clearTimeout(timer);
//   });

  $('.dropdown-el, .dropdown-arrow').click(function(e) {
	//clearTimeout(timer);
    e.preventDefault();
    e.stopPropagation();
    $(this).toggleClass('expanded');
    $('#'+$(e.target).attr('for')).prop('checked',true);
  });
  
  $(document).click(function() {
    $('.dropdown-el, .dropdown-arrow').removeClass('expanded');
  });
    
});  
  function randomIntFromInterval(min,max){
    return Math.floor(Math.random()*(max-min+1)+min);
  };
 ////gd for stop or  on hover/// 
  var itvl = null;
      itvl = window.setInterval(function(){
      $("#refreshproduct").click();
	}, 5000);
  
  $('.product-finder').hover(function() {
    window.clearInterval(itvl);    
	}, function() {
     itvl = window.setInterval(function () {
    	$("#refreshproduct").click();
		}, 5000);
 }); 
