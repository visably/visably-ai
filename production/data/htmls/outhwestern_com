	 jQuery("html, body").animate({ scrollTop: "650px" });
});
jQuery(document).ready(function(){
if(window.pageYOffset != 0) {jQuery('#scroll-down').hide();}
jQuery(window).scroll(function() {
    // User is scrolling
if(window.pageYOffset != 0) {jQuery('#scroll-down').hide();}
else{jQuery('#scroll-down').show();}
});
});
