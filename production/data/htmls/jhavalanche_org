    var apiOptions = false    
    document.addEventListener("DOMContentLoaded", function(){

        //Create map
        cap.init();

        //Tool tips for avy.org link
        var tooltipLink = document.getElementById('avy-open-map-link');
        
        tooltipLink.onmouseover = function(){
            var tooltip = document.getElementById('avy-tooltip');
            tooltip.setAttribute("style","opacity:1;");
        };
        tooltipLink.onmouseout = function(){
            var tooltip = document.getElementById('avy-tooltip');
            tooltip.setAttribute("style","opacity:0;");
        };


    });

