 <script type="text/javascript">
$(document).ready(function() {
   $(".widget-area").hide();

     window.onAmazonLoginReady = function () {

         amazon.Login.setClientId('amzn1.application-oa2-client.0e45952bcb66478d85011939da0fb4a8');
     };

     window.onAmazonPaymentsReady = function () {

         $(".fa-sign-out").on("click", function () {
             amazon.Login.logout();
             document.cookie = "amazon_Login_accessToken=; expires=Thu, 01 Jan 1970 00:00:00 GMT";

         });

         var authRequest;
         var orderReferenceId;

         OffAmazonPayments.Button('AmazonPayButton', "A1AVI5BB4XC2H3", {
             type: "PwA",                 color: "Gold",              size: "small",                 language: "en-GB",              authorization: function () {

                 loginOptions =
                     {
                         scope: "profile postal_code payments:widget payments:shipping_address",
             						                         popup: true
             						                     };

                 authRequest = amazon.Login.authorize(loginOptions, function (v) {
                   $('#amzn-pay-loading').modal("show");
                     var access_token = v.access_token;

 					new OffAmazonPayments.Widgets.AddressBook({
                         sellerId: 'A1AVI5BB4XC2H3',
                         onOrderReferenceCreate: function (orderReference) {
                             orderReferenceId = orderReference.getAmazonOrderReferenceId();
                             $("#order-reference").val(orderReferenceId);
                             $('#amzn_order_ref_id').val(orderReferenceId);
                             $('#amzn_access_token').val(access_token);

                             $.ajax({
                                 type: 'POST',
                                 url: 'https://www.eventdecordirect.com/catalog/amazon_api_call.php?osCsid=pf9tjremaoaq82rb72rgq6mess',
                                 data:{amzn_pay_login:'1',amzn_order_ref_id:orderReferenceId,amzn_access_token:access_token},
                                 cache: false,
                                 dataType:'html',
                                 success: function(res){
                                 if(res)
                                  {
                                    cus_add = JSON.parse(res);
                   								 var checkPage = 'https://www.eventdecordirect.com/catalog/shopping_cart.php?osCsid=pf9tjremaoaq82rb72rgq6mess';
                   								 var checkPageUrl = checkPage + ((checkPage.indexOf('?') == -1) ? '?addr_id='+cus_add.cus_add_id+'&a_id='+cus_add.a_id : '&addr_id='+cus_add.cus_add_id+'&a_id='+cus_add.a_id);
                   								 window.location.href = checkPageUrl;

                                   }
                                 }
                               });
                         },

                         onAddressSelect: function () {

                         },
                         design: {
                             designMode: 'responsive'
                         },
                         onError: function (error) {

                             alert("The following error occurred: "+ error.getErrorCode()+ ' - ' + error.getErrorMessage());
                             console.log(error.getErrorCode() + ': ' + error.getErrorMessage());
                         }
                     }).bind("addressBookWidgetDiv");

                     new OffAmazonPayments.Widgets.Wallet({
                         sellerId: 'A1AVI5BB4XC2H3',
                         onPaymentSelect: function (orderReference) {
                            
                         },
                         design: {
                             designMode: 'responsive'
                         },
                         onError: function (error) {
                             alert("The following error occurred: "+ error.getErrorCode()+ ' - ' + error.getErrorMessage());
                         }
                     }).bind("walletWidgetDiv");

                 });
             },
             onError: function (error) {
               alert("The following error occurred: "+ error.getErrorCode()+ ' - ' + error.getErrorMessage());
               console.log(error.getErrorCode() + ': ' + error.getErrorMessage());

             }
         });

         OffAmazonPayments.Button('AmazonPayButton-cart', "A1AVI5BB4XC2H3", {
             type: "PwA",                 color: "Gold",              size: "medium",                language: "en-GB",              authorization: function () {
                 loginOptions =
                     {
                         scope: "profile postal_code payments:widget payments:shipping_address",
                                                  popup: true
             						                     };

                 authRequest = amazon.Login.authorize(loginOptions, function (v) {
                     $("#access-token").val(v.access_token);
           					var access_token = v.access_token;
                     $('#amzn-pay-loading').modal("show");

                     new OffAmazonPayments.Widgets.AddressBook({
                         sellerId: 'A1AVI5BB4XC2H3',
                         onOrderReferenceCreate: function (orderReference) {
                             orderReferenceId = orderReference.getAmazonOrderReferenceId();
                             $("#order-reference").val(orderReferenceId);
                             $('#amzn_order_ref_id').val(orderReferenceId);
                             $('#amzn_access_token').val(access_token);
                             $.ajax({
                                 type: 'POST',
                                 url: 'https://www.eventdecordirect.com/catalog/amazon_api_call.php?osCsid=pf9tjremaoaq82rb72rgq6mess',
                                 data:{amzn_pay_login:'1',amzn_order_ref_id:orderReferenceId,amzn_access_token:access_token},
                                 cache: false,
                                 dataType:'html',
                                 success: function(res){
                                 if(res)
                                  {
                                    cus_add = JSON.parse(res);
                                      var checkPage = 'https://www.eventdecordirect.com/catalog/shopping_cart.php?osCsid=pf9tjremaoaq82rb72rgq6mess';
                                      var checkPageUrl = checkPage + ((checkPage.indexOf('?') == -1) ? '?addr_id='+cus_add.cus_add_id+'&a_id='+cus_add.a_id : '&addr_id='+cus_add.cus_add_id+'&a_id='+cus_add.a_id);
                                      window.location.href = checkPageUrl;

                                   }
                                 }
                               });
                         },

                         onAddressSelect: function () {

                         },
                         design: {
                             designMode: 'responsive'
                         },
                         onError: function (error) {
                             alert("The following error occurred: "+ error.getErrorCode()+ ' - ' + error.getErrorMessage());
                             console.log(error.getErrorCode() + ': ' + error.getErrorMessage());
                             window.location.href = 'https://www.eventdecordirect.com/catalog/shopping_cart.php?osCsid=pf9tjremaoaq82rb72rgq6mess';
                         }
                     }).bind("addressBookWidgetDiv");

                     new OffAmazonPayments.Widgets.Wallet({
                         sellerId: 'A1AVI5BB4XC2H3',
                         onPaymentSelect: function (orderReference) {

                         },
                         design: {
                             designMode: 'responsive'
                         },
                         onError: function (error) {

                             alert("The following error occurred: "+ error.getErrorCode()+ ' - ' + error.getErrorMessage());
                         }
                     }).bind("walletWidgetDiv");
                 });
             },
             onError: function (error) {
               alert("The following error occurred: "+ error.getErrorCode()+ ' - ' + error.getErrorMessage());
               console.log(error.getErrorCode() + ': ' + error.getErrorMessage());

             }
         });

     };
     });
 </script>
