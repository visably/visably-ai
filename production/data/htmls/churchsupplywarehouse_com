var onestepcheckout_login_popup;
var login_popup;
Event.observe(window, 'load', function() {
    var options = {
        forgot_password_url: 'https://www.churchsupplywarehouse.com/onestepcheckout/ajax/forgotPassword/',
        login_url: 'https://www.churchsupplywarehouse.com/onestepcheckout/ajax/login/',
        translations: {
            invalid_email: 'Please enter a valid email address',
            email_not_found: 'Please enter a registered email address.'
        }
    };
    onestepcheckout_login_popup = new OneStepCheckoutLoginPopup(options);
    login_popup = onestepcheckout_login_popup;
});



</script>
<script>
jQuery(".popup").click(function(){
	
	 
	var guest=jQuery("#login_guest").prop('checked');
	var register=jQuery("#login_register").prop('checked');
	 
	if(guest=='' && register=='')
	{
			alert("Please choose to register or to checkout as a guest")
	}
  if(guest !='')
	{
		 
		window.location.href='https://www.churchsupplywarehouse.com/onestepcheckout'
	}
  if(register !='')
	{
			 
		window.location.href='https://www.churchsupplywarehouse.com/onestepcheckout?ac=1'
	}

});
</script>


