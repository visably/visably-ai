<script type="text/javascript">
    var ecommercePurchaseElements =
        {"product_view":"Product View","side_cart":"Side Cart Button","quick_view":"Quick View","gift_registry":"Gift Registry","saved_for_later":"Saved for Later","wishlist":"Wishlist"},
        ecommercePurchaseRequestParam = "ecommerce_purchase_element",
        insertEcommerceElementToForm = function (form, type) {
            var purchaseInfoEl;
            if (!(purchaseInfoEl = form.down('#ecommerce-purchase-info'))) {
                form.insert(new Element('input', {
                    type  : 'hidden',
                    id    : 'ecommerce-purchase-info',
                    name  : ecommercePurchaseRequestParam,
                    value : ecommercePurchaseElements[type]
                }));
            } else {
                purchaseInfoEl.setValue(ecommercePurchaseElements[type]);
            }
        };
</script>

