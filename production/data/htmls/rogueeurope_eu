<script type="text/javascript">
document.observe("dom:loaded", function(){
	var k = 0,
	    desc = jQuery("p.short-desc");
	if (desc.length > 1) {
		setInterval(function () {
   			k = (k+1) % desc.length;
   			desc.fadeOut().delay(500).eq(k).fadeIn(); 
		}, 5000);
	}
});
</script>
