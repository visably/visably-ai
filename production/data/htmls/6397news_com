    $('.featured-collection__slider').each(function(i, el){
      $(this).flickity({
        cellAlign: 'left',
        contain: true,
        pageDots: false,
        wrapAround: true,
        prevNextButtons: true,
        adaptiveHeight: false,
        imagesLoaded: true,
        on: {
          ready: function(){
            $(this).find('.dot').each(function(i, el){
              $(this).text(i + 1)
            })
          }.bind(this)
        }
      }
    )
    })


