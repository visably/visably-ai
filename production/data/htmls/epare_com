    }
    if(pf_option1 == 2 && pf_option2 == 1){
    	//gift and fun
      epare_product_finder_url = "https://epare.com/collections/lifestyle/products/epare-manual-coffee-grinder";
    }
    if(pf_option1 == 2 && pf_option2 == 2){
    	//gift and spice
      epare_product_finder_url = "https://epare.com/collections/lifestyle/products/epare-manual-coffee-grinder";
    } 
    if(pf_option1 == 2 && pf_option2 == 3){
