<script language="javascript" type="text/javascript">    
    function ExpandMobileShare(id) {
        if (id == 1) { $('.signup-box').addClass("col-xs-12"); $('.signup-box').removeClass("hidden-xs"); $('.close-subscribe').css('display', 'block'); }
        if (id == 2) { $('.wrsocial').addClass("col-xs-12"); $('.wrsocial').removeClass("hidden-xs"); $('.close-social').css('display', 'block'); }
        if (id == 3) { $('.SEEexpand-footer').addClass("seexs-block"); $('.SEEexpand-footer').removeClass("seexs-none"); $('.SEEcollapse-footer').addClass("seexs-none"); $('.SEEcollapse-footer').removeClass("seexs-block"); }
        $("html, body").animate({ scrollTop: $(document).height() }, "slow");
    }
    function CollapseMobileShare(id) {
        if (id == 1) { $('.signup-box').removeClass("col-xs-12"); $('.signup-box').addClass("hidden-xs"); $('.close-subscribe').css('display', 'none'); }
        if (id == 2) { $('.wrsocial').removeClass("col-xs-12"); $('.wrsocial').addClass("hidden-xs"); $('.close-social').css('display', 'none'); }
        if (id == 3) { $('.SEEexpand-footer').addClass("seexs-none"); $('.SEEexpand-footer').removeClass("seexs-block"); $('.SEEcollapse-footer').addClass("seexs-block"); $('.SEEcollapse-footer').removeClass("seexs-none"); }
    }
</script>

            
