<script>
$j(document).click(function (e) {
    e.stopPropagation();
    if($j(e.target).attr('class') == 'header-account-icon'){
        return true;
    }else if($j(e.target).is('a')){
        return true;
    }else{
        $j('#header-account').removeClass('skip-active')
    }
    // console.log($j(e.target).attr('class'));

})

function copyStringToClipboard (str) {
    // Create new element
    var el = document.createElement('textarea');
    // Set value (string to be copied)
    el.value = str;
    // Set non-editable to avoid focus and move outside of view
    el.setAttribute('readonly', '');
    el.style = {position: 'absolute', left: '-9999px'};
    document.body.appendChild(el);
    // Select text inside element
    el.select();
    // Copy text to clipboard
    document.execCommand('copy');
    // Remove temporary element
    document.body.removeChild(el);
    $j('#clipboard-style').remove();
    $j('head').append( '<style id="clipboard-style">.top-promo-coupon-code:before {content: "Copied To Clipboard!" !important; width: 140px !important; left: calc(50% - 72px) !important;}</style>')
}

$j('.top-promo-coupon-code').click(function(){
    copyStringToClipboard('WK13FGM20');
    console.log('copied to clipboard');
});
$j('.top-promo-coupon-code').mouseout(function(){
    $j('#clipboard-style').remove();
});
</script>                <div class="header-center row">
