<script>
$('.dropdown-menu > li > a').mouseenter(function() {
            var curTab = $(this).attr('data-drop-id');
            $('.dropdown-menu a').removeClass('active');
            $(this).addClass('active');
            $('.dropdown2').hide();
            $(curTab).show();
        });
        $('.dropdown-menu').on('mouseleave',function() {
          $('.dropdown-menu a.active').removeClass('active');
          $('.dropdown-menu .dropdown2').hide();
        });
        $('.dropdown2 span.close').on('click',function() {
          $('.dropdown-menu a.active').removeClass('active');
          $('.dropdown-menu .dropdown2').hide();
});
</script>
<script>
$('#cat-926').addClass('cactive');
$('#sub-cat-926').show();
$('.v-dd-menu li').hover(function() {
	$('.h-dd-menu').hide();
	$('.v-dd-menu li').removeClass('cactive');
	var cat_id = this.id;
	$('#'+cat_id).addClass('cactive');
	$('#sub-'+cat_id).show();
});
$('.h-dd-menu > li > a').mouseenter(function() {
    var curTab = $(this).attr('data-drop-id');
    $('.h-dd-menu a').removeClass('active');
    $(this).addClass('active');
    $('.h-dd-menu .dropdown2').hide();
    $(curTab).show();
});
$('.h-dd-menu').mouseleave(function() {
    $('.h-dd-menu a').removeClass('active');
    $('.h-dd-menu .dropdown2').hide();
});
$("li.nav-item.dropdown").hover(function() {
$(this).addClass('nav-active');
});
$("li.nav-item.dropdown").mouseleave(function() {
  $(this).removeClass('nav-active');
});
</script>
