<script type="text/javascript">
  //start by gd for random select
  
 	 $("#refreshproduct").on('click',function(){
  		var pf_option1 = randomIntFromInterval(1,4);
  		var pf_option2 = randomIntFromInterval(1,4);
       if($(window).width() >= 770){
 			$("#pft-1-"+ pf_option1).prop('checked',true);
  	 		$("#pft-2-"+ pf_option2).prop('checked',true);
       }else{
         //var valse1=$("#pf-o-1").val();
         //var valse2=$("#pf-o-1b").val();
         //alert(valse1+"enter into mobile"+valse2);
         //$("#pft-1-"+ pf_option1).prop('checked',true);
  	 		//$("#pft-2-"+ pf_option2).prop('checked',true);
         $('#pf-o-1 option').removeAttr('selected', 'selected');
         $('#pf-o-1b option').removeAttr('selected', 'selected');
        $('#pf-o-1 option').eq(pf_option1).prop('selected', true);
         $('#pf-o-1b option').eq(pf_option2).prop('selected', true);
  	   }
  });
  //end here by gd for random select
  
  $(document).ready(function(){  
  var pf_option1 = randomIntFromInterval(1,4);
  var pf_option2 = randomIntFromInterval(1,4);
  $("#pft-1-"+ pf_option1).prop('checked',true);
  $("#pft-2-"+ pf_option2).prop('checked',true);
  
  var epare_product_finder_url = "";
  $(function(){
    $('.evolve, .product-finder').addClass('animated fadeInDown');
  });

  $('#discover-product').click(function(e){    
	get_product_finder_results(pf_option1, pf_option2);
  });
  
  function get_product_finder_results(pf_option1, pf_option2){
  	if(pf_option1 == 1 && pf_option2 == 1){
	   //timesaver and fun
       epare_product_finder_url = "https://epare.com/collections/lifestyle/products/epeg01";
    }
    if(pf_option1 == 1 && pf_option2 == 2){
      //timesaver and spice
      epare_product_finder_url = "https://epare.com/collections/lifestyle/products/epwe02";
    }
    if(pf_option1 == 1 && pf_option2 == 3){
    	//timesaver and chopping
      epare_product_finder_url = "https://epare.com/collections/spice-herb-mills/products/epspm006";
    }
    if(pf_option1 == 1 && pf_option2 == 4){
    	//timesaver and bright morning
      epare_product_finder_url = "https://epare.com/collections/spice-herb-mills/products/epem001";
