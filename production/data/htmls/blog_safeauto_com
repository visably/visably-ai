<!-- Google Tag Manager (noscript) --><script type="text/javascript" id="sns_global_scripts">// Disable form submit on enter (prevents duplicate trigger)
jQuery(document).keypress(
  function(event){
    if (event.which == '13') {
      event.preventDefault();
    }
});

function doitZip(sourceButton, stateOnly, fromModal) {
    function showError(errorText) {
        alert(errorText);

        return false;
    }

    function getQuoteRedirectUrl(rootUrl, stateId, zipCode) {

        if (/^FL|NJ|NC$/.test(stateId)) {
            _gaq.push(['_trackEvent', 'Home', 'Auto-Quote-Start', 'Home-Auto-PRG-' + stateId]);
            return "https://www.safeauto.com/progressive-redirect";
        }

        if (/^DE|HI|WY$/.test(stateId)) {
            _gaq.push(['_trackEvent', 'Home', 'Auto-Quote-Start', 'Home-Auto-MA', parseInt(zipCode, 10)]);
            return "https://www.safeauto.com/landing/ma-short-flow?userZip=" + zipCode;
        }

        if (/^AK|AR|CT|DC|IA|ID|MA|MD|ME|MI|MN|MS|MT|ND|NE|NH|NM|NV|NY|OR|RI|SD|UT|VT|WA|WI|WV$/.test(stateId)) {
            return "//www.safeauto.com/landing/sure-hits-oof?userZip=" + zipCode + "&stateId=" + stateId;
        }

        //If no state code found
        if (/^XX$/.test(stateId)) {
            _gaq.push(['_trackEvent', 'Home', 'Auto-Quote-Start', 'Home-Auto-OOF', parseInt(zipCode, 10)]);
            return "//www.safeauto.com/landing/sure-hits-oof";
        }

        if (/^CA$/.test(stateId)) {
            return "https://moderator.safeauto.com/v/2/customer/SiteRouting/" + stateId + "/" + zipCode;
            //_gaq.push(['_trackEvent', 'Home', 'Auto-Quote-Start', 'Home-Auto-OOF-CA', parseInt(zipCode, 10)]);
            //return "//www.safeauto.com/landing/sure-hits-ca?userZip=" + zipCode;
        }

        rootUrl = rootUrl.replace(/^(.*?\/\/.*?\/(?:..\/\d{4}\/)).*$/i, "$1").replace(/\/process\/.*?\?__.*$/, "/");

        var url = "https://moderator.safeauto.com/v/2/customer/SiteRouting/" + stateId;

        if (zipCode !== undefined && zipCode !== null)
            url += "/" + zipCode;

        return url;
    }

    function redirectToQuote(rootUrl, stateId, zipCode) {
        var url = getQuoteRedirectUrl(rootUrl, stateId, zipCode);

        window.location.href = url;

        return false;
    }

    function clearDropDownList(ddl) {
        var i = ddl.options.length - 1;

        for (; i > -1; i--)
            ddl.removeChild(ddl.options[i]);
    }

    function createOption(text, selected, value) {
        var e = document.createElement("OPTION");

        if (value !== undefined)
            e.value = value;

        e.selected = selected === undefined ? false : selected;

        e.innerHTML = text;

        return e;
    }

    function addOption(ddl, o) {
        try {
            ddl.options.add(o);
        }
        catch (e) {
            ddl.appendChild(o);
        }
    }

    function populateStatesDropdown(ddl, data) {
        clearDropDownList(ddl);

        ddl.rootUrl = data.rootUrl;
        ddl.zipCode = jQuery("#quoteZip").val();

        addOption(ddl, createOption("Select State", true, "--"));

        for (var i = 0; i < data.states.length; i++)
            addOption(ddl, createOption(data.states[i].StateName, false, data.states[i].StateId));
    }

    function showStateSelectionModal(data) {
        var ddl = jQuery("#ddlStateModal")[0];

        populateStatesDropdown(ddl, data);

        jQuery('#stateModal').modal();
        //$('#stateModal').dialog('open');
    }

    function checkZipCodeLookupResult(data) {
        if (data.states.length < 1) {
            showError("Please enter a valid zip code.");
            jQuery('#quoteZip').focus();
            return false;
        }

        if (data.states.length == 1)
            return redirectToQuote(data.rootUrl, data.states[0].StateId, data.states[0].Zip);

        return showStateSelectionModal(data);
    }

    function doitzipFromModal() {
        var stateId = jQuery("#ddlStateModal").val();

        if (stateId == "--") {
            showError("Please select a state.");
            return false;
        }

        var ddl = jQuery("#ddlStateModal")[0];

        redirectToQuote(ddl.rootUrl, stateId, ddl.zipCode);
    }

    function doitZip2(sourceButton) {
        var zipCode = jQuery("#quoteZip").val().replace(/\s*|\s*$/, "");

        if (!/^[0-9]{5}$/.test(zipCode))
            return showError("Please enter a valid zip code.");


        jQuery(sourceButton).attr("disabled", true);
        jQuery.support.cors = true;
        jQuery.ajax({
            type: "GET",
            url: "https://moderator.safeauto.com/v/2/customer/ZipCodeStateResolver.js",
            data: 'zipCode=' + zipCode,
            dataType: "jsonp",
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                checkZipCodeLookupResult(data);
                jQuery(sourceButton).attr("disabled", false);
            },
            error: function (jqXHR, textStatus, errorThrown) {
                showError("status: " + textStatus + "\r\nerror:" + errorThrown);
            },
            complete: function () {
                jQuery(sourceButton).attr("disabled", false);
            }
        });
    }

    function doitState(quotePath) {
        var stateId = jQuery("#ddlState").val();

        if (stateId == "" || stateId == "Select State")
            return showError("Please select a state.");

        return redirectToQuote(quotePath, stateId);
    }

    if (fromModal)
        doitzipFromModal();

    if (stateOnly)
        return doitState(sourceButton);

    return doitZip2(sourceButton);
}

function doit(quotePath) {
    doitZip(quotePath, true);
}

function doitzipFromModal() {
    doitZip(null, false, true);
