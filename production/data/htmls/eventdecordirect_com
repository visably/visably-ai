$(".product-slider").owlCarousel({
	loop:false,
	autoplay: true,
	autoPlaySpeed: 5000,
	autoPlayTimeout: 5000,
	nav:true,
	navText : ["<i class='fa fa-angle-left'></i>","<i class='fa fa-angle-right'></i>"],
	dots: false,
	items : 5,
	//responsiveClass: true,
    responsive: {
        0:{
          items: 1
