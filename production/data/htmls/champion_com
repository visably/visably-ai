<!--
 Use the showHBIModal(title,contentId,modalClass) function to invoke the modal
 Leave the first parameter, title, empty ('') to omit the header
 The second parameter, contentId, is the wrapper id surrounding the content that you intend to render in the modal
 The contendId wrapper itself will NOT be rendered in the modal; only the innerHTML of the contentId will be cloned
 The contentId wrapper should have a display: none; rule on it; the id you assign is up to you...
 ...as long as the id you assign to the wrapper matches the id you pass in the showHBIModal function
 The third parameter, modalClass, is optional as well; leave empty ('') to omit
 If you provide a modalClass, this will be used to inject a class on the wrapper to enable custom styling
 
 Note: The inline style on the sample anchor (<a>) tag below is not required; it is only intended for this POC

 <a href="javascript:showHBIModal('Optional Title','hbi-modal-poc-content','poc-modal');" style="color:#777;">Show Modal</a>

 <div id="hbi-modal-poc-content" style="display:none;">
 	<div style="text-align:center;">
 		<span style="margin:0 0 10px;display:block;font-size:20px;">FREE SHIPPING WITH $60+ ORDER</span>
 		<span style="clear:left;float:left;">Must order $60+ to receive Free Standard Shipping. No code required. U.S. delivery addresses only.</span>
 	</div>
 </div>
-->
<!-- END - HBI Modal Commentary -->

<!-- BEGIN - HBI Modal JS -->
<script type="text/javascript">
function showHBIModal(title,contentId,modalClass){
	require(["jquery"], function($){
		if($('#hbi-modal-wrapper').length < 1){
			addHBIModal(title,contentId,modalClass);
		} else {
			triggerHBIModal(title,contentId,modalClass);
		}
	});
}

function triggerHBIModal(title,contentId,modalClass){
	require(["jquery"], function($){
		if($("#hbi-modal-wrapper").css("visibility") == "hidden"){
			var hbiModalContent = document.getElementById(contentId).innerHTML;
			$("#hbi-modal-title h3").html(title);
			$("#hbi-modal-content").html(hbiModalContent);
			
			//Hide header if no title
			if(title.trim() == ""){
				$("#hbi-modal").addClass("no-title");
			} else {
				$("#hbi-modal").removeClass("no-title");
			}
			
			//Calculate height
			var hbiModalContentHeight = $("#hbi-modal-content").outerHeight();
			var maxModalContentHeight = $(window).height()*.8;
			
			if(hbiModalContentHeight > maxModalContentHeight){
				$("#hbi-modal-container").height($(window).height()*.8);
			} else {
				$("#hbi-modal-container").height(hbiModalContentHeight);
			}
		
			//Add class to wrapper to enable custom styling
			if(modalClass != ""){
				$("#hbi-modal-wrapper").addClass(modalClass.replace(" ","-"));
			}
		
			$("#hbi-modal-wrapper").css("visibility","visible");
		}
	});
}

function hideHBIModal(){
	require(["jquery"], function($){
		$("#hbi-modal-wrapper").css("visibility","hidden");
		$("#hbi-modal-title h3").html("");
		$("#hbi-modal-content").html("");
		$("#hbi-modal-container").removeAttr("style");
		$("#hbi-modal-wrapper").removeAttr("class");
	});
}

function addHBIModal(title,contentId,modalClass){
	require(["jquery"], function($){
		$('body').append("<div id=\"hbi-modal-wrapper\" style=\"visibility: hidden;\"><div id=\"hbi-modal-mask\"></div><div id=\"hbi-modal-container\"><div id=\"hbi-modal\"><div id=\"hbi-modal-close\"><h3>X</h3></div><div id=\"hbi-modal-title\"><h3></h3></div><div id=\"hbi-modal-content\"></div></div></div></div>");
		$("#hbi-modal-mask").click(function(){
			hideHBIModal();
		});
		$("#hbi-modal-close").click(function(){
			hideHBIModal();
		});
		triggerHBIModal(title,contentId,modalClass);
	});
}
</script>
