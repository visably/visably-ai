<script type="text/ng-template" id="customPopupTemplate.html">
    <div class="custom-popup-wrapper"
         data-ng-style="{left: position().left+'px'}"
         style="display: block;"
         ng-show="isOpen() && !moveInProgress"
         aria-hidden="{{!isOpen()}}">
        <div class="scrollable-wrapper">
    <!-- Product results -->
    <div class="product-suggestions" data-ng-if="(matches | filter: {model: {type: 0}}).length > 0" data-ng-init="$startIndex =  true ? 0 : (matches | filter: {model: {type: 1}}).length">
        <p class="suggestions-title"></p>
        <ul class="suggestions-dropdown-menu" role="listbox">
            <li class="uib-typeahead-match" data-ng-repeat="match in matches | filter: {model: {type: 0}} track by $index" data-ng-class="{active: isActive($startIndex + $index) }"
                data-ng-mouseenter="selectActive($startIndex + $index)" data-ng-click="selectMatch($index)" role="option" id="{{::match.id}}">
                <!-- Default template -->
                <div uib-typeahead-match index="$startIndex + $index" match="match" query="query" template-url="templateUrl"></div>
            </li>
        </ul>
    </div>
    <!-- Article results -->
    <div class="article-suggestions" data-ng-if="(matches | filter: {model: {type: 1}}).length > 0" data-ng-init="$startIndex = true ? (matches | filter: {model: {type: 0}}).length : 0">
        <p class="suggestions-title">Articles</p>
        <ul class="suggestions-dropdown-menu" role="listbox">
            <li class="uib-typeahead-match" data-ng-repeat="match in matches | filter: {model: {type: 1}} track by $index" data-ng-class="{active: isActive($startIndex + $index) }"
                ng-mouseenter="selectActive($startIndex + $index)" data-ng-click="selectMatch((matches | filter: {model: {type: 0}}).length + $index)" role="option" id="{{::match.id}}">
                <a data-ng-href="{{match.model.resultObject.RelativeUrl}}" tab tabindex="-1"
                   data-ng-attr-title="{{match.model.resultObject.Title}}">
                    <img data-ng-src="{{match.model.resultObject.SearchImageAbsoluteUrl}}" data-fallback-src="/images/Responsive/Product/PhotoNotAvailable.png">
                    <span data-ng-bind-html="match.model.resultObject.Title | uibTypeaheadHighlight:query"></span>
                </a>
            </li>
        </ul>
    </div>
        </div>
    </div>
</script>




