<script src="//cdn.shopify.com/s/files/1/0911/2700/t/7/assets/instafeed.min.js?v=9003247094669648496" type="text/javascript"></script>



<script>
	// navigation
$('.dropdown-trigger').mouseover(function() {
	var status = $('.dropdown').css('display');
	if(status=='none') {$('.dropdown').slideDown();}
});
$('.dropdown').mouseleave(function() {
	var status = $('.dropdown').css('display');
	if(status=='block') {$('.dropdown').slideUp();}
});

// Sidebar
$('.static').click(function() {
	var handle = $(this).attr('data');
	$('.subsubnav-'+handle).toggleClass('expanded');
});

// Scroll to top
$('.scroll-to-top').click(function() {
	$("html, body").animate({ scrollTop: 0 }, "slow");
});

var raf = window.requestAnimationFrame ||
	window.webkitRequestAnimationFrame ||
	window.mozRequestAnimationFrame ||
	window.msRequestAnimationFrame ||
	window.oRequestAnimationFrame;

var $shopifyHeader = $('#shopify-section-header');
var $mobileHeader = $('.mobile.super-header');
var $mobileNav = $('.mobile.mobile-nav')
var announceHeight = $('.announcement').outerHeight();
var mobileHeaderHeight = $mobileHeader.outerHeight();
var $window = $(window);
var lastScrollTop = $window.scrollTop();
var threshold = Number.isInteger(announceHeight) ? announceHeight : 50;

$mobileHeader.css({ top: announceHeight });
$mobileNav.css({ top: announceHeight + mobileHeaderHeight });


var mql = window.matchMedia('(max-width: 1100px)');
if (mql.matches && raf) runScrollLoop();
	mql.addListener(function(event) {
	if (event.matches && raf) runScrollLoop();
});

var runScrollEvents = function () {
	var scrollTop = $window.scrollTop();
	var reAnnounceHeight = $('.announcement').outerHeight();
	if (announceHeight !== reAnnounceHeight) {
		threshold = reAnnounceHeight;
		announceHeight = reAnnounceHeight
	}
	if (scrollTop === 0) {
		$mobileNav.css('top', announceHeight + mobileHeaderHeight);
	}
	if (scrollTop >= threshold) {
		$mobileHeader.css('top', 0);
		$shopifyHeader.addClass('sticky')
	} else {
		$mobileHeader.css('top', announceHeight);
		$shopifyHeader.removeClass('sticky');
	}
};

function runScrollLoop() {
	var scrollTop = $window.scrollTop();
	if (lastScrollTop === scrollTop) {
		raf(runScrollLoop);
		return;
	} else {
		lastScrollTop = scrollTop;
		runScrollEvents();
		raf(runScrollLoop);
	}
}

$('.navicon > button').click(function() {
	if ($mobileHeader && $mobileHeader.offset().top > 0) {
		var menuPos = $mobileHeader.offset().top - $(window).scrollTop() + $mobileHeader.outerHeight();
		$mobileNav.css('top', menuPos);
	}
	$('.hamburger').toggleClass('active');
	$('body').toggleClass('locked');
	$mobileNav.toggleClass('open');
});

// Instagram


var userFeed = new Instafeed({
    get:'user',
    userId:'437534151',
	accessToken:'437534151.1677ed0.5958ff577ee34ef8980f2f94ea820166',
	limit:3,
	low_resolution:'low_resolution',
	template:'<a href="{{link}}" target="_blank"><img src="{{image}}" /></a>'
});
//userFeed.run();

	
	
	
	
	
</script>






		
	</body>
