<script>

			var urlpath = 'https://www.churchsupplywarehouse.com/ajaxdata/index/index/';
			jQuery.ajax({
				  url: urlpath,
				  method: 'post',
				  success: function(output) {
					  
					  jQuery('.total-badge').html(output);
                  }
			});
			
			
	jQuery('#tab1show').hide();
jQuery('#tab2show').hide();
jQuery('#tab3show').hide();


jQuery('#header-drop').on('click', 'li', function(event){
    var tabId = jQuery(this).attr('id');
    //jQuery('#tabShows .skip-link-div').hide();
	if(jQuery('#' + tabId + 'show').css('display') == 'block')
	{
		jQuery('#' + tabId + 'show').hide();
	}
	else
	{
		jQuery('#' + tabId + 'show').show();
	}
    
});

jQuery('#tab1').on('click', function(event){
 jQuery('#arw_mini_cart_header').hide();
jQuery('.anchor_mbmenu_text').removeClass('flag'); 
 jQuery('#ms-topmenu').removeClass('active');
 jQuery('#tab2show').hide();
  jQuery('#tab3show').hide();
  jQuery("#tab1").toggleClass("active");
  jQuery('#tab2').removeClass('active');
  jQuery('#tab3').removeClass('active');
   jQuery('#tab4').removeClass('active');
  
});
jQuery('#tab2').on('click', function(event){
  jQuery('#arw_mini_cart_header').hide();
 jQuery('.anchor_mbmenu_text').removeClass('flag'); 
 jQuery('#ms-topmenu').removeClass('active');
  jQuery('#tab1show').hide();
  jQuery('#tab3show').hide();
   jQuery("#tab2").toggleClass("active");
   jQuery('#tab1').removeClass('active');
   jQuery('#tab3').removeClass('active');
   jQuery('#tab4').removeClass('active');
  
});
jQuery('#tab3').on('click', function(event){
   jQuery('#arw_mini_cart_header').hide(); 
 jQuery('#ms-topmenu').removeClass('active');
  jQuery('.anchor_mbmenu_text').removeClass('flag'); 
 jQuery('#ms-topmenu').removeClass('active');
 jQuery('#tab1show').hide();
  jQuery('#tab2show').hide();
  jQuery("#tab3").toggleClass("active");
  jQuery('#tab1').removeClass('active');
  jQuery('#tab2').removeClass('active');
  jQuery('#tab4').removeClass('active');

  
});
jQuery('#tab4').on('click', function(event){
   
 jQuery('#arw_mini_cart_header').toggle();
 jQuery('.anchor_mbmenu_text').removeClass('flag'); 
 jQuery('#tab1').removeClass('active');
 jQuery('#tab2').removeClass('active');
 jQuery('#tab3').removeClass('active');
 jQuery('#ms-topmenu').removeClass('active');
 jQuery('#tab1show').hide();
 jQuery('#tab2show').hide();
 jQuery('#tab3show').hide();
 jQuery("#tab4").toggleClass("active");
  
});

jQuery('#ms-topmenu a').on('click', function(event){
 //jQuery('#ms-topmenu').addClass('active');
 jQuery('#arw_mini_cart_header').hide();
 jQuery(".skip-search-div").css("display","none");
 jQuery('#tab1').removeClass('active');
  jQuery('#tab2').removeClass('active');
 jQuery('#tab3').removeClass('active');
 jQuery('#tab4').removeClass('active');
  jQuery('#tab2show').hide();
 
});

 		
			
		   
