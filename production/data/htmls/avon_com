                            <a class="HightlightedText SalePriceText SeparateLine"
                               data-ng-click="SalePriceClicked($event)">
                                <span class="FinalSaleText" data-ng-if="GetLoadedAvailableProduct(176).IsFinalSale">FINAL SALE </span>
                                <span class="SalePrice">{{ GetProductSalePrice(GetLoadedAvailableProduct(176)) }}</span>
                                <span>when you shop with a Representative&nbsp;»</span>
                            </a>
                    </span>
                </span>
            </span>

            <p data-ng-if="GetLoadedAvailableProduct(176).IsAvailable && GetLoadedAvailableProduct(176).Availability == 7"><span class="AutoReplenishmentPrice">{{GetLoadedAvailableProduct(176).PriceModel.AutoReplenishmentPrice  | currency:&#39;$&#39;}} each shipment</span></p>


            <p class="AvailabilityText" data-ng-if="!GetLoadedAvailableProduct(176).IsAvailable && GetLoadedAvailableProduct(176).AvailabilityDescription === 'No Longer Available'">No Longer Available For Sale</p>
            <p class="OnlineAvailabilityText" data-ng-if="GetLoadedAvailableProduct(176).AvailabilityDescription === 'Not Available On The Online Store'">Not available through the online store at this time</p>
                <p class="HightlightedText NotifyMeClickable" data-ng-show="!GetLoadedAvailableProduct(176).IsAvailable && GetLoadedAvailableProduct(176).AvailabilityDescription == 'Available Soon'">Join the Waitlist</p>

            <p class="ProductSummaryName" data-ng-show="(GetLoadedAvailableProduct(176).HasVariants || GetLoadedAvailableProduct(176).IsMultiVariantProduct) && GetLoadedAvailableProduct(176).IsAvailable">
                <span data-ng-if="GetLoadedAvailableProduct(176).TotalVariantShadesAvailable === 1 && !IsLineNumberSearch">
                    {{ 'Available in 1 shade'.replace('{0}',GetLoadedAvailableProduct(176).TotalVariantShadesAvailable) }}
                </span>
                <span data-ng-if="GetLoadedAvailableProduct(176).TotalVariantShadesAvailable >
                    1">
                    {{ 'Available in {0} shades'.replace('{0}',GetLoadedAvailableProduct(176).TotalVariantShadesAvailable) }}
                </span>
            </p>

            <div class="AutoReplenishmentLabel" data-ng-if="(LoadedProducts[GetLoadedAvailableProduct(176).Id] === undefined && GetLoadedAvailableProduct(176).PriceModel.AutoReplenishmentPrice &gt; 0) || (LoadedProducts[GetLoadedAvailableProduct(176).Id] !== undefined && LoadedProducts[GetLoadedAvailableProduct(176).Id].PriceModel.AutoReplenishmentPrice &gt; 0)">
                <span><i></i>AVON Auto-Replenish</span>
