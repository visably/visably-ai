                   ng-model="searchTerm.value"
                   uib-typeahead="searchResult.value for searchResult in getSearchResultsForTypeahead($viewValue)"
                   typeahead-focus-first="false"
                   typeahead-editable="typeahead-editable"
                   typeahead-min-length="1"
                   typeahead-on-select="onSearchResultSelected($item, $model, $label, $event)"
                   typeahead-wait-ms="250"
                   typeahead-popup-template-url="customPopupTemplate.html"
                   ng-keypress="onKeyPress($event)"
                   focus-me="{{setSearchFocus}}" />
