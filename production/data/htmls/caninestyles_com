<script type="text/javascript">


jQuery(document).ready(function($) {

  var now, lastDatePopupShowed, popupSubbed;
  now = new Date();

  if (localStorage.getItem('lastDatePopupShowed') !== null) {
    lastDatePopupShowed = new Date(parseInt(localStorage.getItem('lastDatePopupShowed')));
  }

  if (localStorage.getItem('popupSubbed') !== null) {
    popupSubbed = localStorage.getItem('popupSubbed');
  }

  	if ((((now - lastDatePopupShowed) >= (15 * 86400000)) || !lastDatePopupShowed) && (popupSubbed !== 'yes')) {
       setTimeout(function() {
          $.magnificPopup.open({
                tClose: 'No Thanks!',
                 items: {
                src: '#mailchimp-popup-form'
              },
              type: 'inline',
              preloader: false,
              focus: '#email',

              // When elemened is focused, some mobile browsers in some cases zoom in
              // It looks not nice, so we disable it:
              callbacks: {
                  beforeOpen: function() {
                      if($(window).width() < 860) {
                          this.st.focus = false;
                      } else {
                          this.st.focus = '#email';
                      }
                  },
                  open: function() {
                    localStorage.setItem('lastDatePopupShowed', now);
                  },
              }
          });
       }, 8000);
    }


    $('.newsletter__submit').on( "click", function() {
      $.magnificPopup.close();
      localStorage.setItem('popupSubbed', 'yes');
    });

  $('#newsletter__no_thanks').on( "click", function() {
  $.magnificPopup.close();
});

});

</script>



  <!-- link that opens popup -->
