        function mobileMenu() {
            var windowWidth = window.innerWidth,
                mobileMenu = '.mobile-menu',
                mainMenu = '.main-menu',
                menuFeatured = '.menu-link-featured',
                headerContent = '.header.content';
            $(window).resize(function () {
                windowWidth = window.innerWidth;
            });

            $body.on('click', mobileMenu, function () {
                if (windowWidth < device.breakpoints.desktop) {
                    $(this).toggleClass('open');
                    $(mainMenu).toggleClass('active');
                    $minicart.toggleClass('minicart-active-zindex');
                    $searchBlock.removeClass('searchbar-active-zindex');
                    $closeCross.removeClass('show-info-search');
                    if ($(mainMenu).hasClass('active')) {
                        $(headerContent).addClass('header-active-zindex');
                        $(document).trigger('deactivateSearchElements');
                        $pageOverlay.addClass('overlay-active');
                        $body.css('overflow', 'hidden');
                        $body.css('height', '100%');
                        $body.css('position', 'relative');
                        $html.css('overflow', 'hidden');
                        $html.css('height', '100%');
                        $html.css('position', 'relative');
                    } else {
                        $(headerContent).removeClass('header-active-zindex');
                        $(mainMenu + ' a').removeClass('active');
                        $('.submenu, .menu-cms-panel').removeClass('active').css('display', '');
                        $pageOverlay.removeClass('overlay-active');
                        $minicartOverlay.removeClass('overlay-active');
                        $body.css('overflow', 'auto');
                        $body.css('height', 'auto');
                        $body.css('position', 'static');
                        $html.css('overflow', 'auto');
                        $html.css('height', 'auto');
                        $html.css('position', 'static');

                        $('.category-item.level0').each(function () {
                            if ($(this).find('.submenu-block').length === 0) {
                                $(this).find('.submenu').css('display', 'none');
                            }
                        });
                    }
                }
            });

            $pageOverlay.on('click', function () {
                $body.css('overflow', 'auto');
                $html.css('overflow', 'auto');
                $html.css('height', 'auto');
                $html.css('position', 'static');
            });

            $body.on('click', mainMenu + ' .parent > a', function (e) {
                if (windowWidth < device.breakpoints.desktop) {
                    e.preventDefault();
                    $(this).toggleClass('active');
                    $(this).siblings('.submenu').toggleClass('active');
                }
            });

            $body.on('click', mainMenu + ' ' + menuFeatured, function (e) {
                if (windowWidth < device.breakpoints.desktop) {
                    e.preventDefault();
                    $(this).toggleClass('active')
                    $(this).next('.submenu').slideToggle(300);
                }
            });

            $body.on('click', mainMenu + ' a.level-top', function () {
                if (windowWidth < device.breakpoints.desktop) {
                    var submenu = $(this).parent().find('.submenu-block > .submenu');
                    if (submenu.length) {
                        submenu.slideToggle(300);
                    } else {
                        $(this).parent().children('.submenu').slideToggle(300);
                    }
                    $(this).siblings('.menu-cms-panel').slideToggle(300);
                }
            });

            $body.on('click', mainMenu + ' .level2 > a', function () {
                if (windowWidth < device.breakpoints.desktop) {
                    $(this).siblings('.submenu').slideToggle(300);
                }
            });

            $body.on('click', mainMenu + ' .level1 > a', function () {
                if (windowWidth < device.breakpoints.desktop) {
                    $(this).siblings('.submenu').slideToggle(300);
                }
            });

            mediaCheck({
                media: '(min-width: 1024px)',
                entry: function () {
                    $(mobileMenu).removeClass('open');
                    $(mainMenu).removeClass('active');
                    $pageOverlay.removeClass('overlay-active');
                    $minicart.removeClass('minicart-active-zindex');

                    $(mainMenu + ' a').removeClass('active');
                    $('.submenu, .menu-cms-panel').removeClass('active').css('display', '');
                },
                exit: function () {
                    $('.category-item > .submenu').each(function () {
                        if ($(this).find('> .submenu-block').length == 0) {
                            $(this).hide();
                        }
                    });
                }
            });
        }

        mobileMenu();
    });

