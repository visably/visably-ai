   });
});

function closeCookie() {
      $('body.openCookieDilog').css('margin-top', '0px !important');
      $('.cookiepop').fadeOut();
      $('.covid-notice').css('padding-bottom', '10px');
      $('body').addClass('cookieAccepted');
