     function addIntlButton(button = 0) {
        var element = document.createElement("button"); 
        element.type = "button";
        element.value = "BUTTON";
        element.name = "intl-button";
      	element.innerHTML = "GLOBAL CHECKOUT <br>(Outside US/CA)";
        element.onclick=  function() { zonosCheckout(); };
         element.className = "intlBtn";
        if(button == 1){
		  element.style = "width: 200px; height: 56px; background-color: #bbb; color: white; border-radius: 5px; margin-right: 5px;";
	    }else{
      	  element.style = "width: 200px; height: 56px; background-color: #bbb; color: white; border-radius: 5px; margin-top: 5px;";
	    }
      
       var cartButton = document.getElementsByClassName("cart-actions");
       cartButton[button].appendChild(element);
    }
function getButtons(){
    var buttons1 = contains('.previewCartCheckout .button--primary', 'checkout'),
        buttons2 = contains('.cart-actions .button--primary', 'Check out'),
        buttons3 = contains('.previewCartAction-checkout', 'Check out now'),
        buttons4 = contains('.ProceedToCheckout .btn', 'Proceed To Checkout'),
        buttons5 = contains('.CheckoutLink', 'Checkout'),
        buttons6 = contains('.button--primary', 'Checkout'),
        buttons7 = contains('.button', 'Check out'),
        buttons8 = contains('.btn', "Proceed To Checkout"),
        buttons9 = contains('.CheckoutButton', "Proceed To Checkout"),
        buttons10 = contains('.CheckoutButton', "PROCEED TO CHECKOUT");
        
    var buttons = buttons1.concat(buttons2, buttons3, buttons4, buttons5, buttons6, buttons7, buttons8, buttons9,buttons10);

    if(buttons){
        for(var i = 0; i < buttons.length; i++){
			console.log("ran");
            buttons[i].addEventListener("click", function(e) { window.stop(); window.location.href = "/checkout.php"; console.log("clicked"); });
        }
    }
};
    
